<script>
    var basemapLayers = [ /* see values in Basemaps.js */
        { 'id': 'osm', 'layer': osmBasemapLayer, 'Leaflet': 'OpenStreetMap' },
        { 'id': 'googleSat', 'layer': googleBasemapLayer, 'Leaflet': 'Google Map' },
        { 'id': 'googleHybrid', 'layer': hybridBasemapLayer, 'Leaflet': 'Google Map' },
        { 'id': 'bingMap', 'layer': bingBasemapLayer, 'Leaflet': 'Google Map' },
    ];

    for (let j = 0; j < basemapLayers.length; j++) {
        if (basemapData[basemapLayers[j].id] === false) {
            $(`#${basemapLayers[j].id}`).attr('disabled', true);

        }
    }

    $("basemap").change(function () {
        let i;
        for (i = 0; i < basemapLayers.length; i++) {
            if (this.id == basemapLayers[i].id) {
                if (basemapData[this.id] === true) {
                    map.addLayer(basemapLayers[i].layer);
                }

            } else {
                if (basemapData[this.id] === true) {
                    map.removeLayer(basemapLayers[i].layer);
                }
            }
        }
    });
</script>
